  "devDependencies": {
    "@types/node": "^20.11.16",
    "husky": "^9.0.10",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
